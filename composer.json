{
	"name": "oops/container-adapter",
	"description": "Types-first PSR-11 container adapter for Nette DI.",
	"keywords": ["nette", "di", "psr-11"],
	"type": "library",
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Jiří Pudil",
			"email": "me@jiripudil.cz",
			"homepage": "https://jiripudil.cz"
		}
	],
	"support": {
		"email": "me@jiripudil.cz",
		"issues": "https://github.com/o2ps/ContainerAdapter/issues"
	},
	"require": {
		"php": "^7.1",
		"nette/di": "^2.4.10 || ^3.0",
		"psr/container": "^1.0"
	},
	"require-dev": {
		"mockery/mockery": "^1.0",
		"nette/bootstrap": "^2.4 || ^3.0",
		"nette/tester": "^2.1"
	},
	"provide": {
		"psr/container-implementation": "1.0.0"
	},
	"autoload": {
		"psr-4": {
			"Oops\\ContainerAdapter\\": "src/"
		}
	},
	"minimum-stability": "beta",
	"prefer-stable": true
}
